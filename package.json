    "element-ui": "^2.11.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "js-cookie": "^2.2.0",
    "node-sass": "^4.9.0",
    "nprogress": "^0.2.0",
    "object-merge-advanced": "^10.10.35",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "indent": [
        2,
        4
      ],
      "space-before-function-paren": 0,
      "keyword-spacing": 0,
      "object-curly-spacing": 0,
      "no-debugger": 0,
      "no-whitespace-before-property": 0,
      "no-multi-spaces": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
